# Test generic constraints parsing

# Entity with generic constraint using 'where' clause
entity Container<T> where T follows Comparable {
    let value: T

    routine compare(other: Container<T>) -> s32 {
        return 0
    }
}

# Record with inheritance constraint
record Wrapper<T> where T from BaseType {
    let data: T
}

# Function with generic constraint
routine max<T>(a: T, b: T) -> T where T follows Comparable {
    return a
}

# Variant with value type constraint
variant Result<T, E> where T: record, E: entity {
    Success(T),
    Error(E)
}

# Protocol with generic constraint
protocol Comparable<T> where T follows Equatable {
    routine compare(other: T) -> s32
}

routine main() -> s32 {
    return 0
}
