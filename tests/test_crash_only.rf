# Test file for @crash_only attribute

# This function should NOT generate try_/check_/find_ variants
@crash_only
routine divide!(a: s32, b: s32) -> s32
{
    if (b == 0)
    {
        throw "Division by zero"
    }
    return a // b
}

# This function SHOULD generate try_/check_/find_ variants (no @crash_only)
routine safe_divide!(a: s32, b: s32) -> s32
{
    if (b == 0)
    {
        throw "Division by zero"
    }
    return a // b
}

routine start()
{
    let result: s32 = divide!(10, 2)
    let result2: s32 = safe_divide!(20, 4)
}
