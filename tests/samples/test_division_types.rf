# Test division and modulo operators for different types

# Integer types: should use // (floor division) and % (modulo)
func test_i32_floor_div() -> i32 {
    return 17i32 // 3i32  # Should generate sdiv for integer floor division
}

func test_i32_modulo() -> i32 {
    return 17i32 % 3i32  # Should generate srem for integer modulo
}

func test_i8_operations() -> i8 {
    return 100i8 // 7i8 + 15i8 % 4i8
}

# Float types: should use / (true division) 
func test_f32_true_div() -> f32 {
    return 17.0f32 / 3.0f32  # Should generate fdiv for float division
}

func test_f64_true_div() -> f64 {
    return 22.7f64 / 3.14f64  # Should generate fdiv for double division
}

# Test mixed operations
func test_complex_expr() -> i64 {
    return 1000i64 // 3i64 * 2i64 + 50i64 % 7i64
}

func main() -> i32 {
    return 0
}