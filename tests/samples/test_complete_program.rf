# Complete RazorForge test program demonstrating all features

recipe add(a: i32, b: i32) -> i32 {
    return a + b
}

recipe test_arithmetic() -> i32 {
    # Same-type arithmetic is allowed
    return 10i32 + 20i32 * 3i32
}

recipe test_return_conversion() -> i8 {
    # Return type casting works
    return 100i32
}

recipe main() -> i32 {
    return add(5i32, test_arithmetic())
}
