# Test extended precision types with simpler values

# 128-bit integers with smaller values
func test_i128() -> i128 {
    return 123456789i128
}

func test_u128() -> u128 {
    return 987654321u128
}

# Extended floating-point
func test_f16() -> f16 {
    return 3.14f16
}

func test_f128() -> f128 {
    return 2.71828f128
}

# Test operations with extended types
func test_i128_ops() -> i128 {
    return 1000i128 + 2000i128 * 3i128 // 5i128
}

func test_u128_ops() -> u128 {
    return 5000u128 - 1000u128 % 13u128
}

func test_f16_ops() -> f16 {
    return 10.5f16 / 2.5f16 + 1.5f16
}

func test_f128_ops() -> f128 {
    return 100.0f128 / 3.0f128 * 2.0f128
}

# Test type constraints: i128/u128 use // and %, f16/f128 use /
func test_constraints() -> i128 {
    return 9999i128 // 7i128 + 100i128 % 3i128
}

func main() -> i32 {
    return 0
}