# Test arrow lambda syntax - parsing only test

routine start() {
    # Test simple arrow lambda: x => expr
    let lambda1 = x => x * 2

    # Test parenthesized arrow lambda: (x) => expr
    let lambda2 = (x) => x * x

    # Test multi-param arrow lambda: (a, b) => expr
    let lambda3 = (a, b) => a + b

    # Test empty params: () => expr
    let lambda4 = () => 42

    return 0
}
