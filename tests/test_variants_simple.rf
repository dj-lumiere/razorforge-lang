# Simple test for variant generation

routine divide(numerator: s32, denominator: s32) -> s32 {
    if denominator == 0 {
        throw DivisionByZeroError()
    }
    return numerator / denominator
}

routine find_item(index: s32) -> s32 {
    if index < 0 {
        absent
    }
    return index
}

routine safe_add(a: s32, b: s32) -> s32 {
    return a + b
}
