record MemorySize {
    private value: uaddr

    public routine __create__(bytes: uaddr) {
        me.value = bytes
    }

    public routine text(memory_size: MemorySize) {
        return f"{me.value.u64()} bytes"
    }

    public routine add(your: MemorySize) {
        return me with { me.value = me.value + your.value }
    }
}
