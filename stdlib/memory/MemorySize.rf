record MemorySize {
    private value: sysuint

    public recipe __create__(bytes: sysuint) {
        me.value = bytes
    }

    public recipe text(memory_size: MemorySize) {
        return f"{me.value.u64()} bytes"
    }

    public recipe add(your: MemorySize) {
        return me with { me.value = me.value + your.value }
    }
}
